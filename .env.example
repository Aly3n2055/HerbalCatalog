# NatureVital Environment Configuration
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/naturevital

# PayPal Configuration (get from PayPal Developer Dashboard)
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here

# Session Security (generate a random string with at least 32 characters)
SESSION_SECRET=your_secure_random_session_secret_key_here

# Application Configuration
NODE_ENV=development
PORT=5000

# Optional: API Base URL for frontend (Netlify deployment)
# VITE_API_BASE_URL=/.netlify/functions

# Optional: Logging and Debug
# LOG_LEVEL=info
# DEBUG=false

# Optional: Rate Limiting
# RATE_LIMIT_WINDOW=900000
# RATE_LIMIT_MAX=100